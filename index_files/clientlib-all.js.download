                if (nextMenu.length > 0) {
                  $('> div:last-child > ul > li:first-child > a',nextMenu).focus(); 
                }
                else {
                    $('.hmb-component-header.visible-md .top-nav .global-links:first :first-child').focus();
                }
            }
        }
