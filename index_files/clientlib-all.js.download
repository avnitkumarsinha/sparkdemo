                var prev = $(this).parent().prev('li');
                if (prev.length>0) {
                    $('> a',prev).focus();
                }
                else {
                    var prevMenu = $(this).closest('.footer-bottom').prev('div');
                    $('> div:last-child > ul > li:last-child > a:last-child',prevMenu).focus();
                }
            }
            else {
