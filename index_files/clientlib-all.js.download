(function() {
  (function($) {
    var Fewlines;
    Fewlines = (function() {

      function Fewlines(el, options) {
        var appendText, div, hidLines, i, lineHeight, needCut, text, visLines, words, _i, _ref,
          _this = this;
        this.defaults = {
          'closeMark': 'close',
