		 	});
			$(".home-card .panel-home p a").each(function(){
				$(this).html($(this).html().replace(/\$47/g,"..."));
			});
			$('.home-tile-content a.openMark').click(function(e){
				$(this).hide();
				$(this).parent().find('span').show();
